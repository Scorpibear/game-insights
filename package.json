{
  "name": "game-insights",
  "description": "Game Insights based on run-time modern opening theory from what was recently played by you at lichess.org/chess.com",
  "private": true,
  "version": "0.5.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "cd dist && mkdir img && cd img && mkdir chesspieces && cd ../.. && cp -r ./img/chesspieces/* ./dist/img/chesspieces/",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "chess.js": "^0.13.4",
    "node-cache": "^5.1.2",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@testing-library/vue": "^6.4.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.29.2",
    "c8": "^7.12.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-vue": "^9.8.0",
    "happy-dom": "^8.9.0",
    "prettier": "^2.8.7",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  }
}
