{
  "name": "game-insights",
  "description": "Game Insights based on run-time modern opening theory from what was recently played by you at lichess.org and chess.com",
  "private": true,
  "version": "0.9.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "cd dist && mkdir img && cd img && mkdir chesspieces && cd ../.. && cp -r ./img/chesspieces/* ./dist/img/chesspieces/",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "chess.js": "^0.13.4",
    "fen-analyzer": "^0.3.1",
    "node-cache": "^5.1.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.3",
    "cypress": "^13.1.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^15.11.6",
    "prettier": "^3.0.1",
    "vite": "^6.0.2",
    "vitest": "^0.34.3"
  }
}
