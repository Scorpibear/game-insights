{
  "name": "game-insights",
  "description": "Game Insights based on run-time modern opening theory from what was recently played by you at lichess.org and chess.com",
  "private": true,
  "version": "0.10.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "cd dist && mkdir img && cd img && mkdir chesspieces && cd ../.. && cp -r ./img/chesspieces/* ./dist/img/chesspieces/",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "chess.js": "^0.13.4",
    "fen-analyzer": "^0.3.1",
    "node-cache": "^5.1.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-v8": "^0.34.6",
    "cypress": "^13.17.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^10.2.0",
    "happy-dom": "^17.4.4",
    "prettier": "^3.5.3",
    "vite": "^4.5.13",
    "vitest": "^3.1.1"
  }
}
