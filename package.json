{
  "name": "game-insights",
  "description": "Game Insights based on run-time modern opening theory from what was recently played by you at lichess.org/chess.com",
  "private": true,
  "version": "0.7.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "cd dist && mkdir img && cd img && mkdir chesspieces && cd ../.. && cp -r ./img/chesspieces/* ./dist/img/chesspieces/",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "coverage": "vitest run --coverage",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "chess.js": "^0.13.4",
    "fen-analyzer": "^0.3.1",
    "node-cache": "^5.1.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@testing-library/vue": "^7.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.1",
    "cypress": "^12.13.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.15.1",
    "happy-dom": "^10.3.2",
    "prettier": "^3.0.0",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  }
}
